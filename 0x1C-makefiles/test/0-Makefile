# Makefile for your project

# Compiler and compiler flags
CC = gcc
CFLAGS = -Wall

# Source files
SOURCES = main.c school.c

# Header files
HEADERS = m.h

# Target executable
TARGET = school

# Rule to build the target executable
$(TARGET): $(SOURCES) $(HEADERS)
	$(CC) $(CFLAGS) $(SOURCES) -o $(TARGET)

# Clean rule to remove generated files
clean:
	rm -f $(TARGET)
